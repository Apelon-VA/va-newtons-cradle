        <exportedArtifact>gov.vha.isaac.ochre.modules:ochre-api</exportedArtifact>
        <exportedArtifact>gov.vha.isaac.ochre.modules:tcc-serializer</exportedArtifact>
        <exportedArtifact>gov.vha.isaac.ochre.modules:tcc-model-impl</exportedArtifact>
        <exportedArtifact>gov.vha.isaac.ochre.modules:tcc-lookup</exportedArtifact>
        <exportedArtifact>gov.vha.isaac.ochre.modules:tcc-api</exportedArtifact>
        <exportedArtifact>gov.vha.isaac.ochre.modules:tcc-dto-ddo</exportedArtifact>
        <exportedArtifact>org.glassfish.hk2</exportedArtifact>
    </exportedArtifacts>
</extension>
